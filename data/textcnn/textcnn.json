{
    "max_length": 100,
    "char_limit": 10,
    "scope":"TextCNN",
    "num_classes":3,

    "with_char": false,
    "char_embedding":"conv",
    "char_emb_dim": 20,
    "char_lstm_dim": 40,

    "batch_size": 32,
    "max_epochs": 1000,
    "dropout_rate": 0.2,
    "learning_rate": 5e-4,
    "optimizer": "adam",
    "lambda_l2": 0.0,
    "grad_clipper": 10.0,

    "context_layer_num": 1,
    "context_lstm_dim": 100,

    "label_type":"all_label",

    // "label_type":"multi_label",

    "slstm_layer_num": 5,

    "rnn":"lstm",
    "random_initialize":false,
    "step":5,
    "slstm_hidden_size":300,
    "weight_decay":1e-5,

    "highway_layer_num": 1,
    "with_highway": true,
    "with_match_highway": true,
    "with_aggregation_highway": true,

    "use_cudnn": false,
    "with_center_loss":false,

    "with_moving_average": 0.999,

    "loss":"focal_loss",

    "alpha":0.5,
    "gamma":2.0,
    "name":"focal_loss_multi_v1",
    "center_gamma":0.001,

    "with_word_drop":false,
    "word_drop_rate":0.7,

    "scale":30,
    "margin":0.35,

    "l2_normalization":false,

    "elmo_pca":false,

    "num_filters":128,
    "max_pool_size":5,
    "encoder_type":"textcnn",
    "num_blocks":2,
    "hidden_units":300,
    "num_heads":4,

    "output_folder_name":"textcnn",
    "with_target":false,
    "trainable_embedding":false
}
